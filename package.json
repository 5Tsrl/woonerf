{
  "name": "@conveyal/woonerf",
  "description": "React/Redux bootstrapping and common libs for Conveyal",
  "main": "index.js",
  "repository": {
    "url": "https://github.com/conveyal/woonerf",
    "type": "git"
  },
  "scripts": {
    "cover": "yarn test -- --coverage --coverage-paths \"src/**/*.js\"",
    "clean": "rm -r actions components reducers store ./*.js ./*.js.map",
    "prepublish": "mastarm prepublish src --outdir ./",
    "pretest": "yarn && yarn clean",
    "test": "mastarm lint && mastarm test -e test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "Trevor Gerhardt <trevorgerhardt@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "auth0-lock": "^10.19.0",
    "debug": "^2.6.8",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "react-addons-perf": "^15.4.2",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "mastarm": "^3.11.0",
    "nock": "^9.0.14",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.3.0",
    "semantic-release": "^6.3.6"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "peerDependencies": {
